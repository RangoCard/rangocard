{% set route = app.request.attributes.get('_route') %}
<!DOCTYPE HTML>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>RangoCard</title>

    <meta name="description" content="overview &amp; stats" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no" />
    <meta http-equiv="content-language" content="pt-br" />
    <meta name="description" content="Rangocard uma plataforma para fidelização de cliente" />
    <meta name="keywords" content="rango, card, rangocard, fidelização, fidelizar" />
    <meta name="author" content="Rangocard" />
    <meta name="company" content="Rangocard" />
    <meta property="og:description" content="Cadastra-se e conheça o Rangocard, uma plataforma para fidelização de cliente! Lembra do cartão fidelidade? Nós somos a solução digital.">
    <meta property="og:locale"      content="pt_BR">
    <meta property="og:site_name"   content="Rangocard">
    <meta property="og:type"        content="website">
    <link rel="canonical" href="https://www.rangocard.com.br/" />
    {% block metas %}
    {% endblock %}
    <link rel="icon" href="{{ asset('assets/images/favicon-rango.ico') }}" type="image/x-icon">

    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('assets/images/icones/icone-144x144.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" type="image/x-icon" href="{{ asset('assets/images/icones/icone-114x114.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('assets/images/icones/icone-72x72.png') }}">
    <link rel="apple-touch-icon-precomposed" href="{{ asset('assets/images/icones/icone-57x57.png') }}">
    <!-- bootstrap & fontawesome -->
    <link rel="stylesheet" href="{{ asset('assets/css/libs/bootstrap.min.css') }}" />
    <link rel="stylesheet" href="{{ asset('assets/css/libs/font-awesome/4.5.0/css/font-awesome.min.css') }}" />

    <!-- ace styles -->
    <link rel="stylesheet" href="{{ asset('assets/css/libs/ace-compilado.min.css') }}" class="ace-main-stylesheet" id="main-ace-style" />

    {% block customstylesheets %}
    {% endblock %}
</head>
<body {% block body_attrs %}{% endblock %}>
{% for type, flash_messages in app.session.flashBag.all %}
    {% for flash_message in flash_messages %}
        <div class="alert alert-{{ type }} flashbag-message" role="alert" style="display:none;">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            {{ flash_message }}
        </div>
    {% endfor %}
{% endfor %}
{# LOADING #}
<div id="globalLoading">
    <img src="{{ asset('assets/images/loading.svg') }}" alt="Carregando..."/>
</div>

{% block header %}
    {% include '@App/header.html.twig' %}
{% endblock %}

{% block content %}
{% endblock %}

<!--[if !IE]> -->
<script src="{{ asset('assets/js/libs/jquery-2.1.4.min.js') }}"></script>
<!-- <![endif]-->
<!--[if IE]>
<script src="{{ asset('assets/js/libs/jquery-1.11.3.min.js') }}"></script>
<![endif]-->
<script type="text/javascript">
    if('ontouchstart' in document.documentElement) document.write("<script src='{{ asset('assets/js/libs/jquery.mobile.custom.min.js') }}'>"+"<"+"/script>");
</script>
<script type="text/javascript" src="{{ asset('assets/js/libs/bootstrap.min.js') }}"></script>

<script src="{{ asset('assets/js/libs/holder.min.js') }}"></script>
<script src="{{ asset('assets/js/libs/bootstrap-editable.min.js') }}"></script>
<script src="{{ asset('assets/js/libs/jquery.raty.min.js') }}"></script>
<!-- ace scripts -->
<script src="{{ asset('assets/js/libs/ace-elements.min.js') }}"></script>
<script src="{{ asset('assets/js/libs/ace.min.js') }}"></script>
<script>
    var notificationsUrl = '{{ path('user_notifications') }}';
    var removeNotificationUrl = '{{ path('user_remove_notification') }}';
    var currentLocalUrl = '{{ path('user_current_local') }}';
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCa1gB1AvPzOjnWk3CqEmUW9mU4uWYSCPE" type="text/javascript"></script>
<script src="{{ asset('assets/js/geolocalization.js') }}"></script>
<script src="{{ asset('assets/js/notifications.js') }}"></script>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-46634024-3', 'auto');
  ga('send', 'pageview');

</script>
{% block customjavascripts %}
{% endblock %}
</body>
</html>